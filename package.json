{
  "name": "enterprise-erp-system",
  "version": "1.0.0",
  "description": "Modular Enterprise Resource Planning (ERP) System",
  "main": "server/index.js",
  "scripts": {
    "dev": "concurrently \"npm run server:dev\" \"npm run client:dev\" \"npm run analytics:dev\"",
    "server:dev": "nodemon server/index.js",
    "client:dev": "cd client && npm run dev",
    "analytics:dev": "cd analytics && python manage.py runserver",
    "build": "cd client && npm run build",
    "start": "node server/index.js",
    "test": "jest",
    "setup": "npm install && cd client && npm install && cd ../analytics && pip install -r requirements.txt"
  },
